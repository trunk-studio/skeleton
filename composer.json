{
    "name": "trunk-studio/aws-sns-laravel-notification",
    "description": "aws-sns-laravel-notification",
    "homepage": "https://github.com/trunk-studio/aws-sns-laravel-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Trunksys",
            "email": "service@trunk-studio.com",
            "homepage": "https://trunk-studio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AwsSns\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
